{% for multimedia in multimediaList %}
    <ul class="media-list objects">
        {{ include('CMBundle:Multimedia:object.html.twig') }}
    </ul>
{% endfor %}

{% if multimediaList.paginationData is defined and multimediaList.paginationData.current < multimediaList.paginationData.last %}
    <div class="well well-small load_more">
        <a href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params') | merge({ 'page': multimediaList.getPaginationData.next })) }}">{{ 'Load more' | trans }}</a>
    </div>
{% endif %}