<li class="object" data-type="multimedia" data-id="{{ multimedia.id }}">

    {% if multimedia.type == constant('CM\\CMBundle\\Entity\\Multimedia::TYPE_YOUTUBE') %}
        <a href="//www.youtube.com/embed/{{ multimedia.source }}" ilightbox="multimedia">
            <img width="100%" height="450px" src="http://img.youtube.com/vi/{{ multimedia.source }}/0.jpg" />
        </a>
        {#<iframe width="100%" height="450px" src="//www.youtube.com/embed/{{ multimedia.source }}" frameborder="0" allowfullscreen></iframe>#}
    {% elseif multimedia.type == constant('CM\\CMBundle\\Entity\\Multimedia::TYPE_VIMEO') %}
        <a href="//player.vimeo.com/video/{{ multimedia.source }}?color=040505" ilightbox="multimedia">
            <img width="100%" height="450px" src="{{ vimeoImage(multimedia.source) }}" />
        </a>
        {#<iframe src="//player.vimeo.com/video/{{ multimedia.source }}?color=040505" width="100%" height="450px" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>#}
    {% elseif multimedia.type == constant('CM\\CMBundle\\Entity\\Multimedia::TYPE_SOUNDCLOUD') %}
        <a href="//w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/{{ multimedia.source }}&amp;color=000000&amp;auto_play=false&amp;show_artwork=true" ilightbox="multimedia">
            <img width="100%" height="450px" src="{{ soundcloudImage(multimedia.source) }}" />
        </a>
        {#<iframe width="100%" height="166" scrolling="no" frameborder="no" src="//w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/{{ multimedia.source }}&amp;color=000000&amp;auto_play=false&amp;show_artwork=true"></iframe>#}
    {% endif %}


</li>