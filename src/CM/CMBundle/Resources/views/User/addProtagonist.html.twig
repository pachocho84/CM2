{% if protagonistNewId is not defined %}
    {% set protagonistNewId = 0 %}
{% endif %}
{% for field in entityUsers %}
    {% if skip is not defined or loop.index0 >= protagonistNewId %}
        <div protagonist_new_id="{{ loop.index0 }}" class="collection-item protagonists_user" user_id="{{ user_ids[loop.index0 - protagonistNewId] }}" {% if target is defined %}{{ target[0] }}="{{target[1] }}"{% endif %}>
            <div class="form-group"><a class="pull-right btn btn-default protagonists_remove" id="protagonists_remove_{{ loop.index0 }}">{{ "Remove" | trans }}</a></div>
            {{ form_errors(field) }}
            {{ form_widget(field) }}
        </div>
    {% endif %}
{% endfor %}