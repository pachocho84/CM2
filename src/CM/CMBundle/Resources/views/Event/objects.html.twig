<pre>{% for key, param in dates.getPaginationData %}
	{{ key }}: {{ param }}<br/>
{% endfor %}</pre>



{% for key, date in dates %}

	{% if key == 0 or date.start | date('Y/m/d') != dates[key-1].start | date('Y/m/d') %}
	
		{% if "#{dates.getPaginationData.current}_#{key}" != '1_0' %}
			</ul>
		{% endif %}
		
		<div class="box-top">
			<h2><i class="glyphicon glyphicon-calendar"></i> {{ date.start | format_date(null, null, null, constant('IntlDateFormatter::LONG')) }}</h2>
		</div>
		
		<ul class="media-list objects">
	{% endif %}
	
	{{ include('CMBundle:Event:object.html.twig' ) }}
	
{% endfor %}
		</ul>



{% if dates.paginationData.current < dates.paginationData.last %}
	<div id="load_more_posts" class="well well-small">
		<a href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params') | merge({'page': app.request.attributes.get('_route_params').page ? app.request.attributes.get('_route_params').page + 1 : 2})) }}">{{ 'Load more' | trans }}</a>
	</div>
{% endif %}