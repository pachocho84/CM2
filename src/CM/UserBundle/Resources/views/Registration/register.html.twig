<div>
    <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
        <div class="row">
            <div class="col-sm-6">
                {{ form_row(form.firstName) }}
            </div>
            <div class="col-sm-6">
                {{ form_row(form.lastName) }}
            </div>
        </div>
        {{ form_widget(form) }}
        <div class="clearfix">
            <input type="submit" value="{{ 'Register'|trans }}" class="btn btn-primary pull-right" />
        </div>
    </form>
</div>